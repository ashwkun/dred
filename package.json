    "@heroicons/react": "^2.2.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.3.1",
    "framer-motion": "^12.6.3",
    "lottie-react": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.1",
    "tailwind-animatecss": "^1.0.0",
    "tesseract.js": "^6.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@parcel/optimizer-terser": "^2.13.3",
    "@parcel/packager-raw-url": "^2.13.3",
    "@parcel/transformer-webmanifest": "^2.13.3",
    "autoprefixer": "^10.4.20",
    "buffer": "^6.0.3",
    "parcel": "^2.13.3",
    "postcss": "^8.5.2",
    "process": "^0.11.10",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.17"
  },
